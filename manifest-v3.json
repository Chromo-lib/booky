{
  "manifest_version": 3,
  "name": "Booky",
  "description": "Beautiful and modern new tab page for most modern browsers.",
  "version": "2.0.1",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "./icons/16.png",
    "32": "./icons/32.png",
    "48": "./icons/48.png",
    "64": "./icons/64.png",
    "128": "./icons/128.png",
    "256": "./icons/256.png"
  },
  "permissions": [
    "bookmarks",
    "topSites"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}