{
    "manifest_version": 2,
    "name": "Booky",
    "description": "New tab page for most modern browsers.",
    "version": "1.0.0",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "./icons/16.png",
        "32": "./icons/32.png",
        "48": "./icons/48.png",
        "64": "./icons/64.png",
        "128": "./icons/128.png",
        "256": "./icons/256.png"
    },
    "permissions": [
        "activeTab",
        "chrome://favicon/*"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self';"
}